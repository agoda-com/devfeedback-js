{
  "name": "agoda-devfeedback-rsbuild",
  "version": "2.0.7",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "agoda-devfeedback-common": "workspace:*",
    "tslib": "2.8.1",
    "ws": "8.18.1"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@rsbuild/core": "1.3.5",
    "@rspack/core": "1.3.4",
    "@types/ws": "8.18.1"
  },
  "peerDependencies": {
    "@rsbuild/core": ">=1.0.0",
    "@rspack/core": ">=1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "https://github.com/agoda-com/devfeedback-js"
  }
}
